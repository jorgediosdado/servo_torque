#include "ros/ros.h"
#include "std_msgs/String.h"
#include "std_msgs/Float64.h"
#include "sensor_msgs/JointState.h"

/**
 * This tutorial demonstrates simple receipt of messages over the ROS system.
 */

std_msgs::Float64 msgsdata;
double position;	
double force;




int main(int argc, char **argv)
{


  ros::init(argc, argv, "publisher");
  ros::NodeHandle n;


  //ros::Subscriber sub = n.subscribe("/desired_position", 1000, publisher);
  ros::Publisher chatter_pub = n.advertise<std_msgs::Float64>("/flexipod/front_left_body_coxa/command", 2, true);
 
  while (ros::ok())
  {

      
   	 //ROS_ERROR("force is %d",force);
   	 msgsdata.data=2;
    	 chatter_pub.publish(msgsdata);
	 ros::spinOnce();
 }

	
	
  return 0;
}
